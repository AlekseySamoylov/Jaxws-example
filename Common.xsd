    <xs:simpleType name="Quality">
      <xs:restriction base="xs:string">
        <xs:enumeration value="FSS" />
        <xs:enumeration value="FTY" />
        <xs:enumeration value="INS" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="StockCategory">
      <xs:restriction base="xs:string">
        <xs:enumeration value="O" />
        <xs:enumeration value="C" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="NeedKind">
      <xs:restriction base="xs:string">
        <xs:enumeration value="ORDER" />
        <xs:enumeration value="MMOBILE" />
        <xs:enumeration value="S2S" />
        <xs:enumeration value="PICKUP" />
        <xs:enumeration value="PICKUP_T" />
        <xs:enumeration value="RIM" />
        <xs:enumeration value="B2B" />
        <xs:enumeration value="LOGISTIC" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="SystemId">
      <xs:restriction base="xs:string">
        <xs:enumeration value="NTS" />
        <xs:enumeration value="CRM" />
        <xs:enumeration value="RTD" />
        <xs:enumeration value="Mmobile" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ErrorLevel">
      <xs:restriction base="xs:string">
        <xs:enumeration value="ERROR" />
        <xs:enumeration value="WARNING" />
        <xs:enumeration value="LOGIC" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="Flag">
      <xs:restriction base="xs:string">
        <xs:enumeration value="Y" />
        <xs:enumeration value="N" />
        <xs:enumeration value="" />
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="CustomerAttributeKey">
      <xs:restriction base="xs:string">
